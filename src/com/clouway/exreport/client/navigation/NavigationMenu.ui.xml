<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

  <ui:style src="navigation.css"/>

  <g:StackLayoutPanel ui:field="sideMenu"  addStyleNames="{style.stackPanel}">
    <g:stack>
      <g:customHeader size="50" >
        <g:Label>Expenses</g:Label>
      </g:customHeader>
      <g:SimplePanel addStyleNames="{style.sampleContactsPanel}">
        <g:VerticalPanel addStyleNames="{style.anchorsVerticalPanel}">

          <g:Anchor ui:field="todayExpenses" text="today"/>
          <g:Anchor ui:field="thisWeekExpenses" text="this week"/>
          <g:Anchor ui:field="thisMonthExpenses" text="month"/>
        </g:VerticalPanel>
      </g:SimplePanel>

    </g:stack>
    <g:stack>
      <g:customHeader size="50">
        <g:Label>Account</g:Label>
      </g:customHeader>
      <g:Label>Edit account</g:Label>
    </g:stack>
  </g:StackLayoutPanel>

</ui:UiBinder>