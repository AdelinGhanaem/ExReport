<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:myStuff='urn:import:com.clouway.exreport.client.expensesdashboard.view'
             xmlns:c='urn:import:com.google.gwt.user.cellview.client'>


    <ui:style src="dashboard.css">


    </ui:style>


    <g:HTMLPanel>
        <g:HorizontalPanel>
            <g:DecoratorPanel addStyleNames="{style.decoratorPanel}">
                <g:ScrollPanel ui:field="cellTreeScrollPanel" addStyleNames="{style.scrollPanel}">
                    <g:HorizontalPanel ui:field="cellTreePanel">
                        <c:CellTree ui:field="cellTree">
                        </c:CellTree>
                    </g:HorizontalPanel>
                </g:ScrollPanel>
            </g:DecoratorPanel>
            <g:VerticalPanel>
                <g:DecoratorPanel addStyleNames="{style.cellTreeDecoratorPanel}">
                    <g:ScrollPanel ui:field="cellTableScrollPanel" addStyleNames="{style.CellTreeScrollPanel}">
                        <g:HorizontalPanel>
                            <c:CellTable ui:field="expensesCellTable">
                            </c:CellTable>
                        </g:HorizontalPanel>
                    </g:ScrollPanel>
                </g:DecoratorPanel>


            </g:VerticalPanel>


        </g:HorizontalPanel>

    </g:HTMLPanel>


</ui:UiBinder>