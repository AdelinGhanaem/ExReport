<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:mine='urn:import:com.clouway.exreport.client.expensesreporting.addingexpenses.view'
             xmlns:b='urn:import:com.github.gwtbootstrap.client.ui'>

  <ui:style src="style.css"/>

  <g:HTMLPanel addStyleNames="{style.addingExpenseContainer}">
    <g:FlowPanel addStyleNames="{style.form}" ui:field="addExpenses">
      <g:Label text="Add expense" addStyleNames="{style.lable}"/>
      <mine:ExpenseEditor ui:field="expenseEditor"/>
      <b:Button ui:field="save" type="SUCCESS" text="save"/>
    </g:FlowPanel>
  </g:HTMLPanel>
</ui:UiBinder>